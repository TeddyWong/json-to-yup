{
  "name": "j2y",
  "version": "0.1.14",
  "description": "JSON to Yup or Yup as config lets you save Yup validation as YAML or JSON.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/*.test.ts",
    "publish npm": "npm --no-git-tag-version version patch; cd dist; npm publish",
    "build": "tsc",
    "cleanup": "rm -Rf dist; mkdir dist; cd dist; ln -s ../package.json"
  },
  "keywords": [
    "yup",
    "json"
  ],
  "author": "Herman Geldenhuys(modified by Teddy Wong)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TeddyWong/json-to-yup"
  },
  "homepage": "https://github.com/TeddyWong/json-to-yup",
  "dependencies": {
    "@types/node": "^17.0.21",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.0",
    "@types/yup": "^0.29.13",
    "chai": "^4.3.6",
    "npm-check-updates": "^12.4.0",
    "nyc": "^15.1.0",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.5.0"
  }
}
